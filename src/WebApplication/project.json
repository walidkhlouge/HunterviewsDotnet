{
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "copyToOutput": {
      "include": [
        "wwwroot",
        "**/*.cshtml",
        "appsettings.json",
        "web.config"
      ],
      "exclude": [
        "bin",
        "obj"
      ]
    },
    "preserveCompilationContext": true
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "**/*.cshtml",
      "appsettings.json",
      "web.config"
    ],
    "exclude": [
      "bin",
      "obj"
    ]
  },

  "dependencies": {
    "Microsoft.Extensions.Configuration": "1.0.1",
    //"Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.EntityFrameworkCore": "1.0.0",
    "MySql.Data.Core": "7.0.4-IR-191",
    "MySql.Data.EntityFrameworkCore": "7.0.4-IR-191",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.2",
    "Microsoft.AspNetCore.Routing": "1.0.2",
    "Microsoft.AspNetCore.Mvc": "1.0.2",
    "Microsoft.AspNetCore.Razor.Tools": {
      "version": "1.0.0-preview4-final",
      "type": "build"
    },
    "Microsoft.AspNetCore.StaticFiles": "1.0.1",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.1",
    "Microsoft.Extensions.Configuration.Json": "1.0.1",
    "Microsoft.Extensions.Logging": "1.0.1",
    "Microsoft.Extensions.Logging.Console": "1.0.1",
    "Microsoft.Extensions.Logging.Debug": "1.0.1",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.3"
    },
        "Novell.Directory.Ldap.NETStandard": "2.3.0",
    "Microsoft.AspNetCore.Diagnostics": "1.0.1",
    "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.1",
    "Microsoft.EntityFrameworkCore.InMemory": "1.1.1",
    "Microsoft.AspNetCore.Session": "1.1.0",
    "Stormpath.AspNetCore": "0.9.0",
    "Microsoft.AspNetCore.Cors": "1.1.1",
    "Microsoft.AspNetCore.Mvc.WebApiCompatShim": "1.0.1"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {

      },
      "imports": [
        "dnxcore50",
        "portable-net452+win81"
      ]
    }


  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },

  "scripts": {
    "precompile": [ "dotnet bundle" ]
  },

  "tools": {
    "BundlerMinifier.Core": "2.2.301",
    "Microsoft.AspNetCore.Razor.Tools": "1.0.0-preview4-final"
  },

  "version": "1.0.3"





}
